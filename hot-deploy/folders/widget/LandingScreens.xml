			</actions>
			<widgets>
				<decorator-screen name="main-decorator" location="${parameters.mainDecoratorLocation}">
					<decorator-section name="body">
						<platform-specific>
							<html><html-template location="component://folders/webapp/folders/landing/taxShop.ftl"/></html>
						</platform-specific>
