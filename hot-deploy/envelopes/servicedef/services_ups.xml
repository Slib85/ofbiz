<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<description>Envelopes.com Services</description>
	<vendor>Envelopes.com</vendor>
	<version>1.0</version>

	<service name="getTimeInTransit" engine="java" max-retry="0" location="com.envelopes.ups.UPSServices" transaction-timeout="1200" invoke="getTimeInTransit" auth="true">
		<description>Store the time in transit for given shipment method types</description>
		<attribute name="postalCode" type="String" mode="IN" optional="true"/> <!-- Zip codes that start with these characters will be processed -->
		<attribute name="stateProvinceGeoId" type="String" mode="IN" optional="true"/> <!-- state geo id to process, should be 1 state at a time -->
	</service>
</services>