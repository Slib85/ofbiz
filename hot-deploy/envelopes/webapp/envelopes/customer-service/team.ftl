		<script type="text/javascript">
			$(document).ready(function() {
				$('.jqs-employee').on('mouseover', function() {
					if (!$(this).find('.employee_text_container').is(':visible')) {
						$(this).find('img').attr('src', $(this).find('img').attr('src').replace('.jpg', '') + '_hover.jpg');
						$(this).find('.employee_text_container').show();
					}
				}).on('mouseleave', function() {
					if ($(this).find('.employee_text_container').is(':visible')) {
						$(this).find('img').attr('src', $(this).find('img').attr('src').replace('_hover.jpg', '') + '.jpg');
						$(this).find('.employee_text_container').hide();
					}
				});
			});
		</script>
