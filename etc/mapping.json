{
	"settings":{
		"analysis":{
			"char_filter": {
				"special_symbols": {
					"type": "mapping",
					"mappings": [
						"#=>hashtag\\u0020",
						"w-2=>w2",
						"W-2=>W2",
						"z-fold=>zfold",
						"Z-fold=>Zfold"
					]
				},
				"special_symbols_reg": {
					"type": "pattern_replace",
					"pattern": "(?i)w\\-2",
					"replacement": "w2"
				}
			},
			"filter":{
				"autocomplete_filter": {
					"type": "edge_ngram",
					"min_gram": 1,
					"max_gram": 15
				},
				"english_stop":{
					"type":"stop",
					"stopwords":"_english_"
				},
				"english_stemmer": {
					"type": "porter_stem",
					"language": "english"
				},
				"english_possessive_stemmer": {
					"type": "porter_stem",
					"language": "possessive_english"
				},
				"ngram_filter": {
					"type": "nGram",
					"min_gram":3,
					"max_gram":5
				},
				"synonym_filter" : {
					"type" : "synonym_graph",
					"synonyms" : [
						"lux lining,foil",
						"cd,dvd",
						"a7,5 x 7",
						"seed,mini",
						"metallic,mettallic,shimmer,sparkle,glitter",
						"policy,booklet",
						"coin,jeweler,jewelry,tip,money",
						"pochette,petals",
						"holiday,christmas,xmas",
						"holiday invitations,christmas party invitations",
						"labels,seals",
						"digital sheets,13 x 19 paper,12 x 18 paper",
						"medical,x-ray,expansion",
						"airmail,express,rush priority",
						"glassine,translucent,vellum,clear,see through",
						"currency,ticket",
						"savoy,cotton",
						"fuschia,fuchsia,fushcia",
						"church,remittance,offering,fundraising,donation,charity",
						"photo,photograph,picture",
						"flyers,business announcement",
						"rfid,credit card sleeve,secureshield,rf blocking",
						"head shot,full face",
						"sale,clearance",
						"w-2,w2,w 2",
						"z fold,zfold,z-fold",
						"table tents,place cards=>#17 mini",
						"barronial=>baronial",
						"interoffice=>clasp",
						"padded=>bubble"
					]
				}
			},
			"tokenizer": {
				"autocomplete_tokenizer": {
					"type": "edge_ngram",
					"min_gram": 1,
					"max_gram": 15,
					"token_chars": [
						"letter",
						"digit"
					]
				}
			},
			"analyzer": {
				"autocomplete_analyzer": {
					"type": "custom",
					"tokenizer": "autocomplete_tokenizer",
					"filter": [
						"standard",
						"lowercase"
					]
				},
				"brand_analyzer": {
					"type": "custom",
					"char_filter": [
						"special_symbols"
					],
					"tokenizer": "standard",
					"filter": [
						"standard",
						"lowercase",
						"english_stop"
					]
				},
				"standard_analyzer": {
					"type": "custom",
					"char_filter": [
						"special_symbols"
					],
					"tokenizer": "standard",
					"filter": [
						"standard",
						"lowercase",
						"synonym_filter",
						"english_stop"
					]
				},
				"simplesize_analyzer": {
					"type": "custom",
					"char_filter": [
						"special_symbols"
					],
					"tokenizer": "keyword",
					"filter": [
						"standard",
						"lowercase",
						"synonym_filter"
					]
				},
				"size_analyzer": {
					"type": "custom",
					"char_filter": [
						"special_symbols"
					],
					"tokenizer": "whitespace",
					"filter": [
						"standard",
						"lowercase",
						"synonym_filter"
					]
				},
				"sku_analyzer": {
					"type": "custom",
					"tokenizer": "whitespace",
					"filter": [
						"standard",
						"lowercase"
					]
				},
				"stem_gram_analyzer": {
					"type": "custom",
					"char_filter": [
						"special_symbols"
					],
					"tokenizer": "standard",
					"filter": [
						"standard",
						"lowercase",
						"synonym_filter",
						"english_possessive_stemmer",
						"english_stop",
						"english_stemmer",
						"ngram_filter"
					]
				},
				"stem_analyzer": {
					"type": "custom",
					"char_filter": [
						"special_symbols"
					],
					"tokenizer": "standard",
					"filter": [
						"standard",
						"lowercase",
						"synonym_filter",
						"english_possessive_stemmer",
						"english_stop",
						"english_stemmer"
					]
				},
				"ngram_analyzer": {
					"type": "custom",
					"char_filter": [
						"special_symbols"
					],
					"tokenizer": "standard",
					"filter": [
						"standard",
						"lowercase",
						"synonym_filter",
						"english_possessive_stemmer",
						"english_stop",
						"ngram_filter"
					]
				}
			}
		}
	},
